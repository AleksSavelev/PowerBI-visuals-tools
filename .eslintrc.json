{
    "env": {
        "es6": true,
        "node": true
    },
	"parser": "@typescript-eslint/parser",
    "extends": "eslint:recommended",
    "plugins": [
        "@typescript-eslint"
    ],
    "parserOptions": {
        "ecmaVersion": 2023,
        "sourceType": "module",
	    "project": "tsconfig.json",
	    "tsconfigRootDir": "."
    },
    "rules": {
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": "error"
    }
}